<!--
This file gets relatively imported based on SolutionDir
So we can change per-solution settings.
-->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="12.0">
  <PropertyGroup>
    <PackageDir>$(SolutionDir)..\packages\</PackageDir>
    <Godot>Yes</Godot>
  </PropertyGroup>
</Project>
