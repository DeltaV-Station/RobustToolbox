<Control xmlns="https://spacestation14.io"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         x:Class="Robust.Client.UserInterface.ProfTreeEntry"
         MouseFilter="Stop">
    <BoxContainer Orientation="Vertical">
        <BoxContainer Orientation="Horizontal">
            <ExpanderArrow Access="Public" Name="Arrow" MinSize="12 12" VerticalAlignment="Center" />
            <Label Name="Text" Access="Public" HorizontalExpand="True" ClipText="True" Margin="4" />
        </BoxContainer>
        <BoxContainer Orientation="Vertical" Name="ChildEntryContainer" Access="Public" Margin="16 0 0 0" />
    </BoxContainer>
</Control>
